datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Menu {
  id        String     @id @default(cuid())
  name      String
  menuItems MenuItem[]
}

model MenuItem {
  id     String @id @default(cuid())
  name   String
  price  Float
  menu   Menu   @relation(fields: [menuId], references: [id])
  menuId String
}
